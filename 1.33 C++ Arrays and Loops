#include <iostream>

int main() {
    // Declare and initialize an array
    int numbers[5] = {1, 2, 3, 4, 5};

    // Access and print array elements using a for loop
    std::cout << "Array elements: ";
    for (int i = 0; i < 5; ++i) {
        std::cout << numbers[i] << " ";
    }
    std::cout << std::endl;

    // Modify array elements using a for loop
    for (int i = 0; i < 5; ++i) {
        numbers[i] *= 2;
    }

    // Print modified array elements using a for loop
    std::cout << "Modified array elements: ";
    for (int i = 0; i < 5; ++i) {
        std::cout << numbers[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
