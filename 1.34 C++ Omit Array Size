#include <iostream>

int main() {
    // Declare and initialize an array without specifying the size
    int numbers[] = {1, 2, 3, 4, 5};

    // Access and print array elements
    std::cout << "Array elements: ";
    for (int i = 0; i < 5; ++i) {
        std::cout << numbers[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
