#include <iostream>

// Function with multiple parameters
int add(int a, int b) {
    return a + b;
}

int main() {
    // Declare variables
    int num1 = 5;
    int num2 = 3;

    // Call the function with multiple parameters
    int sum = add(num1, num2);

    // Print the result
    std::cout << "Sum: " << sum << std::endl;

    return 0;
}
