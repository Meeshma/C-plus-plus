#include <iostream>

// Function to add two integers
int add(int a, int b) {
    return a + b;
}

// Function to add three integers
int add(int a, int b, int c) {
    return a + b + c;
}

// Function to add two double values
double add(double a, double b) {
    return a + b;
}

int main() {
    // Call the overloaded functions
    std::cout << "Sum of 5 and 3: " << add(5, 3) << std::endl;
    std::cout << "Sum of 5, 3, and 2: " << add(5, 3, 2) << std::endl;
    std::cout << "Sum of 5.5 and 3.3: " << add(5.5, 3.3) << std::endl;

    return 0;
}
