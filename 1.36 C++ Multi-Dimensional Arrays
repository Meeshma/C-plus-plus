#include <iostream>

int main() {
    // Declare and initialize a 2D array
    int matrix[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    // Access and print elements of the 2D array
    std::cout << "2D Array elements:" << std::endl;
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            std::cout << matrix[i][j] << " ";
        }
        std::cout << std::endl;
    }

    // Modify elements of the 2D array
    matrix[0][0] = 10;
    matrix[2][2] = 90;

    // Print modified elements of the 2D array
    std::cout << "Modified 2D Array elements:" << std::endl;
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            std::cout << matrix[i][j] << " ";
        }
        std::cout << std::endl;
    }

    return 0;
}
